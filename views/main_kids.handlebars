          <li>1970</li>
          <li>1980</li>
          <li>1990</li>
        </ul>
      </div>
      <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
        {{#each index}}
        <a href="/details/{{ con_id }}"><img src="/images/movies/{{con_image}}" alt="{{con_name}}"></a>
        {{/each}}

