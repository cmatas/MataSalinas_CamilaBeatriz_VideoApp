<div class="container">
  <div class="row header">
    <div class="col-xs-12 col-sm-12">
        <video autoplay>
          <source src="/images/video/{{con_video}}" type="video/mp4">
        </video>
    </div>
    <div class="col-xs-12 col-sm-12">
      {{#each details}}
      <h3>{{con_name}}</h3>
      <p>{{ con_desc }}</p>
      {{/each}}
    </div>
  </div>
</div>
